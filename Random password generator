# 隨機密碼產生器
import random
import string
number = string.digits
english = string.ascii_letters
alphabet = list(number + english)  # 轉為清單
random.shuffle(alphabet)
PWlength = int(input("how many digits of password do you want?"))
password = "".join(alphabet[:PWlength])  # 整理成字串
print(f"your password is: {password}")
